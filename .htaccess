RewriteEngine on




RewriteRule ^([a-z]+){1,3}\/([a-z_]+)\/([A-Z足a-z足0-9足-]+)$ index.php?lang=$1&page=$2&id=$3 [L]
RewriteRule ^([a-z]+){1,3}\/([a-z_]+)\/$ index.php?lang=$1&page=$2 [L]
RewriteRule ^([a-z]+){1,3}\/([a-z_]+)$ index.php?lang=$1&page=$2 [L]


RewriteRule ^([a-z]+){1,3}\/([0-9足])$ index.php?lang=$1&id=$2 [L]
RewriteRule ^([a-z]+){1,3}\/$ index.php?lang=$1 [L]